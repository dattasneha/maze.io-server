generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    id        String   @id @default(uuid())
    email     String?
    name      String?
    password  String?
    isGuest   Boolean
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
